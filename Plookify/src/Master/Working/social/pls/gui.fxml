<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: #383838;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Master.Working.social.pls.GuiController">
   <children>
      <Text fill="WHITE" layoutX="75.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="lookify">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Text>
      <ImageView fitHeight="55.0" fitWidth="56.0" layoutX="20.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Scratch/ICONS/logo.png" />
         </image>
      </ImageView>
      <SplitPane layoutX="179.0" layoutY="14.0" prefHeight="487.0" prefWidth="805.0" style="-fx-background-color: #383838;">
         <items>
            <AnchorPane prefHeight="507.0" prefWidth="803.0">
               <children>
                  <TextField fx:id="searchField" layoutX="567.0" layoutY="14.0" promptText="Search" style="-fx-background-radius: 20;">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
                  <TableView fx:id="results" layoutX="14.0" layoutY="54.0" prefHeight="384.0" prefWidth="226.0" stylesheets="@../GUI/test.css">
                    <columns>
                      <TableColumn fx:id="user" prefWidth="212.0" text="User" />
                    </columns>
                  </TableView>
                  <AnchorPane fx:id="friendView" layoutX="231.0" layoutY="54.0" prefHeight="384.0" prefWidth="280.0" style="-fx-background-color: #ffffff;">
                     <children>
                        <TableView fx:id="friendList" prefHeight="384.0" prefWidth="282.0" stylesheets="@../GUI/test.css">
                          <columns>
                            <TableColumn fx:id="friends" prefWidth="135.0" text="Friends" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
                  <Button fx:id="addFriend" layoutX="595.0" layoutY="159.0" mnemonicParsing="false" onMouseClicked="#launchAdded" style="-fx-background-color: #383838; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="+" textFill="WHITE">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="makePrivate" layoutX="577.0" layoutY="398.0" mnemonicParsing="false" onMouseClicked="#launchPrivate" style="-fx-background-color: #383838; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="Private" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="removeFriend" layoutX="595.0" layoutY="108.0" mnemonicParsing="false" onMouseClicked="#launchDialog" style="-fx-background-color: #383838; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="--" textFill="WHITE">
                     <font>
                        <Font size="12.0" />
                     </font>
                  </Button>
                  <AnchorPane fx:id="confirmDialog" layoutX="264.0" layoutY="166.0" prefHeight="139.0" prefWidth="216.0" style="-fx-background-color: #323232;">
                     <children>
                        <Label layoutX="14.0" layoutY="29.0" text="Are you sure you want to delete?" textFill="WHITE">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <Button fx:id="confirmYes" layoutX="57.0" layoutY="70.0" mnemonicParsing="false" onMouseClicked="#yesDelete" style="-fx-background-color: #343434; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="Yes" textFill="WHITE">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Button fx:id="confirmNo" layoutX="122.0" layoutY="70.0" mnemonicParsing="false" onMouseClicked="#noDelete" style="-fx-background-color: #343434; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="No" textFill="WHITE">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="privateDialog" layoutX="261.0" layoutY="165.0" prefHeight="139.0" prefWidth="216.0" style="-fx-background-color: #343434;">
                     <children>
                        <Label layoutX="11.0" layoutY="32.0" text="Are you sure you want to private?" textFill="WHITE">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <Button fx:id="yesPrivate" layoutX="57.0" layoutY="67.0" mnemonicParsing="false" onMouseClicked="#goPrivate" style="-fx-background-color: #343434; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="Yes" textFill="WHITE">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Button>
                        <Button fx:id="noPrivate" layoutX="122.0" layoutY="67.0" mnemonicParsing="false" onMouseClicked="#stayPublic" style="-fx-background-color: #343434; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="No" textFill="WHITE">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="friendAddedDialog" layoutX="262.0" layoutY="165.0" prefHeight="139.0" prefWidth="216.0" style="-fx-background-color: #343434;">
                     <children>
                        <Label layoutX="51.0" layoutY="31.0" text="Friend Request sent" textFill="WHITE">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                        <Button layoutX="91.0" layoutY="70.0" mnemonicParsing="false" onMouseClicked="#acceptDialog" style="-fx-background-color: #343434; -fx-border-color: #ffffff; -fx-border-radius: 20;" text="Ok" textFill="WHITE">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
               </children>
            </AnchorPane>
         </items>
      </SplitPane>
   </children>
</AnchorPane>
